{
  "name": "movie-night-data-worker",
  "version": "1.0.0",
  "description": "A template for kick starting a Cloudflare Workers project",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write '**/*.{js,css,json,md}'",
    "build": "webpack --mode=production --env.runtime=webworker",
    "dev": "webpack --mode=development --watch --env.runtime=node",
    "analyze": "webpack --mode=development --watch --env.analyze=true --env.runtime=webworker"
  },
  "author": "Kevin Kipp <kevin.kipp@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "husky": "^2.1.0",
    "prettier": "^1.17.0",
    "webpack-bundle-analyzer": "^3.3.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format",
      "pre-push": "npm run format"
    }
  },
  "dependencies": {
    "@babel/core": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-typescript": "^7.3.3",
    "@types/jws": "^3.2.0",
    "@types/mime": "^2.0.1",
    "@types/node-fetch": "^2.3.7",
    "babel-loader": "^8.0.6",
    "dotenv": "^8.0.0",
    "googleapis": "^41.0.0",
    "json-loader": "^0.5.7",
    "jsrsasign": "^10.5.25",
    "node-fetch": "^2.6.0",
    "typescript": "^3.5.3",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.5"
  }
}
